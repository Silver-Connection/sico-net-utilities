{
    "metadata": [
      {
        "src": [
          {
              "files": [
                  "src/SiCo.Utilities.Compression/*.csproj",
                  "src/SiCo.Utilities.Crypto/*.csproj",
                  "src/SiCo.Utilities.CSV/*.csproj",
                  "src/SiCo.Utilities.Generics/*.csproj",
                  "src/SiCo.Utilities.Helper/*.csproj",
                  "src/SiCo.Utilities.Pgsql/*.csproj",
                  "src/SiCo.Utilities.Web.Charts/*.csproj",
                  "src/SiCo.Utilities.Web.Vue/*.csproj"
              ],
            "exclude": [
              "**/obj/**",
              "**/bin/**",
              "_site/**"
            ]
          }
        ],
        "dest": "docfx/api",
        "properties": {
          "TargetFramework": "netstandard2.0"
        }
      }
    ],
    "build": {
      "content": [
        {
          "src": "docfx",
          "files": [
            "api/**.yml",
            "api/index.md"
          ]
        },
        {
          "src": "docfx",
          "files": [
            "articles/**.md",
            "articles/**/toc.yml",
            "toc.yml",
            "*.md"
          ],
          "exclude": [
            "obj/**",
            "docs/_site/**"
          ]
        }
      ],
      "resource": [
        {
          "src": "docfx",
          "files": [
            "images/**"
          ],
          "exclude": [
            "obj/**",
            "docs/_site/**"
          ]
        }
      ],
      "overwrite": [
        {
          "src": "docfx",
          "files": [
            "apidoc/**.md"
          ],
          "exclude": [
            "obj/**",
            "docs/_site/**"
          ]
        }
      ],
      "dest": "docs/",
      "globalMetadataFiles": [],
      "fileMetadataFiles": [],
      "globalMetadata": {
        "_appTitle": "Silver Connection Utilities",
        "_appFooter": "Copyright Â© 2015-2017 Silver Connection <small>Generated by <strong><a href='https://dotnet.github.io/'>DocFX</a></strong></small>",
        "_appLogoPath": "images/logo.svg",
        "_FaviconPath": "images/favicon.ico"
      },
      "template": [
        "default"
      ],
      "postProcessors": [],
      "noLangKeyword": false
    }
  }